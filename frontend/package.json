{
  "name": "@cabe-arena/frontend",
  "version": "1.0.0",
  "description": "Cabe Arena Frontend - React application with Vite and TypeScript",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules",
    "analyze": "vite-bundle-analyzer",
    "audit": "npm audit --audit-level=moderate",
    "lighthouse": "lighthouse http://localhost:5173 --output=json --output-path=./lighthouse-report.json",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "accessibility": "axe-core --dir src --format json --output accessibility-report.json"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.2",
    "@types/react-datepicker": "^6.2.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-calendar-heatmap": "^1.9.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.2",
    "react-router-dom": "^6.20.1",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cabe-arena/eslint-config": "file:../shared/eslint-config",
    "@cabe-arena/ts-config-base": "file:../shared/ts-config-base",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.43",
    "@types/react-calendar-heatmap": "^1.9.0",
    "@types/react-dom": "^18.2.17",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.7.2",
    "cssnano": "^6.0.1",
    "eslint": "^8.54.0",
    "eslint-plugin-import": "^2.32.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^11.3.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "vite": "^5.0.8",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-compression2": "^0.10.0",
    "vite-plugin-pwa": "^0.17.4"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "npm@>=10"
}
